# Server Configuration
PORT=3001
NODE_ENV=development

# Frontend CORS
FRONTEND_ORIGIN=http://localhost:5173

# Clerk Authentication
# Get both keys from https://dashboard.clerk.com
CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key_here

# Database
SQLITE_PATH=./data/maswada.db

# OpenAI Configuration
# Get your API key from https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your_openai_api_key_here

# OpenAI Organization ID (optional, only needed if you have multiple organizations)
# Get it from https://platform.openai.com/account/organization
OPENAI_ORGANIZATION_ID=org-your_organization_id_here

# OpenAI Model (optional, defaults to gpt-5-mini)
# Options: gpt-5-mini, gpt-5, gpt-4o-mini, gpt-4o, gpt-4-turbo, etc.
OPENAI_MODEL=gpt-5-mini
